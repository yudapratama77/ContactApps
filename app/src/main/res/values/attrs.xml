<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ViewPager">
        <attr name="swipeDisabled" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="ExpandableLayout">
        <attr name="el_headerLayout" format="reference"/>
        <attr name="el_contentLayout" format="reference"/>
        <attr name="el_duration" format="integer"/>
    </declare-styleable>

    <declare-styleable name="RTCGlView">
        <attr name="mainMirror" format="boolean"/>
        <attr name="secondMirror" format="boolean"/>
        <attr name="mainCoords" format="reference"/>
        <attr name="secondCoords" format="reference"/>
    </declare-styleable>

    <declare-styleable name="CameraView">
        <attr name="android:adjustViewBounds"/>
        <attr name="facing" format="enum">
            <enum name="back" value="0"/>
            <enum name="front" value="1"/>
        </attr>
        <attr name="cameraRatio" format="string"/>
        <attr name="autoFocus" format="boolean"/>
        <attr name="flash" format="enum">
            <enum name="off" value="0"/>
            <enum name="on" value="1"/>
            <enum name="torch" value="2"/>
            <enum name="auto" value="3"/>
            <enum name="redEye" value="4"/>
        </attr>
    </declare-styleable>
</resources>